<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
	<os:ShortName>Neuros.TV</os:ShortName>
	<os:Description>Neuros.TV search</os:Description>
	<os:InputEncoding>UTF-8</os:InputEncoding>
	<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
	<SearchForm>http://dev.neuros.tv/videos</SearchForm>
	<os:Url type="text/html" method="POST" template="http://app.neuros.tv/videos/index/">
		<os:Param name="plugin" value="1"/>
		<os:Param name="search" value="{searchTerms}"/>
	</os:Url>
	<os:Url type="application/x-suggestions+json" method="POST" template="http://app.neuros.tv/shows/search/exact/0/mode/json/keywords/{searchTerms}">
	</os:Url>
</SearchPlugin>

